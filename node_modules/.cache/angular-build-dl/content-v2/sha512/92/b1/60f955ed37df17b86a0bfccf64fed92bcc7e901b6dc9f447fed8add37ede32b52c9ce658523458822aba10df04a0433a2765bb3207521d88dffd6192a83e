ltoPDF() {\r\n    this.spinner.show();\r\n    this.emailattchementurl.length = 0;\r\n    this.emailattchementurl = [];\r\n    // parentdiv is the html element which has to be converted to PDF\r\n    html2canvas(document.querySelector(\"#content\")).then(canvas => {\r\n\r\n      var pdf = new jsPDF('p', 'pt', [canvas.width, canvas.height]);\r\n\r\n      var imgData = canvas.toDataURL(\"image/jpeg\", 1.0);\r\n      pdf.addImage(imgData, 0, 0, canvas.width, canvas.height);\r\n\r\n      var pdf1 = pdf.output('blob');\r\n      var file = new File([pdf1], \"PO_\" + this.PoNumber + \".pdf\");\r\n\r\n      let body = new FormData();\r\n      debugger\r\n      body.append('Dan', file);\r\n      this.pomservice.UploadPO(file).subscribe(res => {\r\n        ;\r\n        this.emailattchementurl.push(res);\r\n        this.sendmail1()\r\n        debugger\r\n      });\r\n\r\n      // pdf.save('PO.pdf');\r\n\r\n    });\r\n\r\n  }\r\n\r\n\r\n\r\n  message: any;\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n  public emailattchementurl = [];\r\n  public sendmail1() {\r\n    debugger\r\n    // \"C:\\\\inetpub\\\\wwwroot\\\\digiPOAPI\\\\Images\\\\PO\\\\20210326085908PO.pdf\"\r\n    this.emailattchementurl.push(\"D:\\\\inetpub\\\\wwwroot\\\\digiPOAPI\\\\Images\\\\PO\\\\Termsandconditions.pdf\")\r\n    var entity = {\r\n      'emailto': this.vendoremail,\r\n      'emailsubject': 'PO_from Tops:' + this.PoNumber,\r\n      'emailbody': \"Dear (\" + this.suppliernme + \"),<br><br><br>\" + \"Attached is the Tops Markets Purchase Order along with applicable Terms and Conditions. Feel free to contact us if you have any questions or concerns.<br>\" + \"<br><br><br>Thank You  <br>\" + this.authorizer + \"<br>Tops Purchasing Dept<br>direct line: \" + this.primaryphoneno + \"<br>fax: \" + this.faxtelephone + \" <br>\" + this.Authorizeremail,\r\n      'attachmenturl': this.emailattchementurl\r\n      // this.emailattchementurl\r\n    }\r\n    debugger\r\n    this.pomservice.sendemail1(entity).subscribe(data => {\r\n
ock;\\r\\n}\\r\\n\\r\\n.sidenav a:hover {\\r\\n  color: #00b5d7;\\r\\n  cursor: pointer;\\r\\n}\\r\\n\\r\\n.togglebtn{\\r\\n /* background-color: #343536; */\\r\\n border: none;\\r\\n color: aliceblue;\\r\\n font-size: 15px;\\r\\n padding-left: 7%;\\r\\n width: 100%;\\r\\n text-align: left;\\r\\n}\\r\\n\\r\\n.tab {\\r\\n  overflow: hidden;\\r\\n  border: 1px solid #ccc;\\r\\n  background-color: #f1f1f1;\\r\\n}\\r\\n\\r\\n.tab a {\\r\\n  background-color: inherit;\\r\\n  float: left;\\r\\n  border: none;\\r\\n  outline: none;\\r\\n  cursor: pointer;\\r\\n  width: 100%;\\r\\n  /* padding: 14px 16px;\\r\\n  transition: 0.3s;\\r\\n  font-size: 17px; */\\r\\n}\\r\\n\\r\\n/* .tab a:hover {\\r\\n  background-color: #006096;\\r\\n} */\\r\\n\\r\\n.tab a.active {\\r\\n  background-color: #00b5d7;\\r\\n  color: white;\\r\\n  width: 100%;\\r\\n}\\r\\n\\r\\n/*# sourceMappingURL=data:application/json;base64, */\"","import { Component, OnInit } from '@angular/core';\r\nimport { PomsService } from '../../poms.service';\r\nimport Swal from 'sweetalert2';\r\nimport { ActivatedRoute } from '@angular/router';\r\n@Component({\r\n  selector: 'app-sidebar',\r\n  templateUrl: './sidebar.component.html',\r\n  styleUrls: ['./sidebar.component.css']\r\n})\r\nexport class SidebarComponent implements OnInit {\r\n\r\n  constructor(public pomservice: PomsService) { }\r\n  public roleid:any;\r\n  public user:any;\r\n\r\n  ngOnInit() {\r\n    this.roleid=localStorage.getItem('roleid');\r\n    this.user=localStorage.getItem('user');\r\n  }\r\n\r\n\r\n  public highlight(evt) {\r\n    var i, tablinks;\r\n   \r\n    tablinks = document.getElementsByClassName(\"tablinks\");\r\n    for (i = 0; i < tablinks.length; i++) {\r\n      tablinks[i].className = tablinks[i].className.replace(\" active\", \"\");\r\n    }\r\n    \r\n    evt.currentTarget.className += \" active\";\r\n  }\r\n}\r\n","export default \"\\n/*# sourceMappingURL=data:application/json;base64,eyJ2ZXJzaW9uIjozLCJzb3VyY2VzIjpbXSwibmFtZXMiOltdL
    let BillToMasterComponent = class BillToMasterComponent {
      constructor(pomservice, activatedroute) {
        this.pomservice = pomservice;
        this.activatedroute = activatedroute;
      }

      ngOnInit() {
        this.activatedroute.params.subscribe(params => {
          this.id = params['id'];

          if (this.id != undefined || this.id != null) {
            this.showbit = 0;
            this.getbillmaster();
          } else {
            this.showbit = 1;
          }
        });
        this.getcountrymaster();
        this.GetDepartment();
        this.stateid = "";
        this.cityid = "";
        this.countryid = "";
        this.departmentid = "";
      }

      GetDepartment() {
        this.pomservice.GetDepartmentMaster().subscribe(data => {
          this.departmentlist = data;
        });
      }

      getcountrymaster() {
        this.pomservice.GetCountryMaster().subscribe(data => {
          this.countrylist = data;
        }, error => {});
      }

      GetCountryID(even) {
        this.countryid = even.target.value;
        this.GetStateMasterbycountryid();
      }

      GetStateMasterbycountryid() {
        this.pomservice.GetStateMasterByCountryID(this.countryid).subscribe(data => {
          this.statelist = data;
        }, error => {});
      }

      GetStateID(even) {
        this.stateid = even.target.value;
        this.GetCityMasterbyStateid();
      }

      GetCityMasterbyStateid() {
        this.pomservice.GetCityMasterByStateID(this.stateid).subscribe(data => {
          this.citylist = data;
        }, error => {});
      }

      GetCityID(even) {
        this.cityid = even.target.value;
      }

      GetDepartmentID(even) {
        this.departmentid = even.target.value;
      }

      insertdetails() {
        var entity = {
          'ClientCode': this.clientcode,
          'ClientName': this.clientname,
          'Street': this.street,
          'Address': this.address,
          'CityID': this.cityid,
          'StateID': this.stateid,
          'ZipCode': this.zipcode,
          'CountryID': this.countryid,
          'PrimaryPhoneNo': this.primaryphoneno,
          'SecondaryPhoneNo': this.secondaryphoneno,
          'Email': this.email,
          'FaxTelephone': this.faxtelephone,
          'bidept': this.departmentid
        };
        this.pomservice.InsertBillToMaster(entity).subscribe(data => {
          if (data != 0) {
            sweetalert2__WEBPACK_IMPORTED_MODULE_3___default.a.fire('Completed', 'Data Added Successfully');
            this.clear();
            location.href = "#/BillToMasterDashboard";
          }
        });
      }

      clear() {
        this.clientcode = "", this.clientname = "", this.street = "", this.address = "", this.city = "", this.state = "", this.zipcode = "", this.country = "", this.secondaryphoneno = "", this.faxtelephone = "", this.email = "";
        this.primaryphoneno = "";
      }

      getbillmaster() {
        this.pomservice.GetBillToMaster().subscribe(data => {
          this.billlist = data;
          var bill = this.billlist.filter(x => x.id == this.id);
          this.clientcode = bill[0].clientCode, this.clientname = bill[0].clientName, this.street = bill[0].street, this.address = bill[0].address, this.cityid = bill[0].cityID, this.stateid = bill[0].stateID, this.zipcode = bill[0].zipCode, this.countryid = bill[0].countryID, this.primaryphoneno = bill[0].primaryPhoneNo, this.secondaryphoneno = bill[0].secondaryPhoneNo, this.email = bill[0].email, this.faxtelephone = bill[0].faxTelephone, this.departmentid = bill[0].bidept, this.GetStateMasterbycountryid();
          this.GetCityMasterbyStateid();
          this.GetDepartment();
        }, error => {});
      }

      UpdateDetails() {
        var entity = {
          'ID': this.id,
          'ClientCode': this.clientcode,
          'ClientName': this.clientname,
          'Street': this.street,
          'Address': this.address,
          'CityID': this.cityid,
          'StateID': this.stateid,
          'ZipCode': this.zipcode,
          'CountryID': this.countryid,
          'PrimaryPhoneNo': this.primaryphoneno,
          'SecondaryPhoneNo': this.secondaryphoneno,
          'Email': this.email,
          'FaxTelephone': this.faxtelephone,
          'bidept': this.departmentid
        };
        this.pomservice.UpdateBillToMaster(entity).subscribe(res => {
          let data = res;
          sweetalert2__WEBPACK_IMPORTED_MODULE_3___default.a.fire('Success', 'Data Updated Successfully');
          this.getbillmaster();
          location.href = "#/BillToMasterDashboard";
        });
      }

    };


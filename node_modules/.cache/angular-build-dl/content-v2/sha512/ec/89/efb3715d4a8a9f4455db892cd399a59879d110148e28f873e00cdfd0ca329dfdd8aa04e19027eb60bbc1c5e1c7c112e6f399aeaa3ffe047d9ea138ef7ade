    let FobMasterComponent = class FobMasterComponent {
      constructor(pomservice, activatedroute) {
        this.pomservice = pomservice;
        this.activatedroute = activatedroute;
      }

      ngOnInit() {
        this.activatedroute.params.subscribe(params => {
          this.id = params['id'];

          if (this.id != undefined || this.id != null) {
            this.showbit = 0;
            this.GetFobMaster();
          } else {
            this.showbit = 1;
          }
        });
      }

      insertdetails() {
        var entity = {
          'FobCode': this.fobcode,
          'FobDescription': this.fobcdesc
        };
        this.pomservice.InsertFOBMaster(entity).subscribe(data => {
          if (data != 0) {
            sweetalert2__WEBPACK_IMPORTED_MODULE_3___default.a.fire('Completed', 'Data Added Successfully');
            location.href = "#/FobMasterDashboard";
            this.clear();
          }
        });
      }

      clear() {
        this.fobcode = "", this.fobcdesc = "";
      }

      GetFobMaster() {
        this.pomservice.GetFOBMaster().subscribe(data => {
          this.foblist = data;
          var fob = this.foblist.filter(x => x.id == this.id);
          this.fobcode = fob[0].fobCode, this.fobcdesc = fob[0].fobDescription;
        }, error => {});
      }

      UpdateDetails() {
        var entity = {
          'ID': this.id,
          'FobCode': this.fobcode,
          'FobDescription': this.fobcdesc
        };
        this.pomservice.UpdateFOBMaster(entity).subscribe(res => {
          let data = res;
          sweetalert2__WEBPACK_IMPORTED_MODULE_3___default.a.fire('Completed', 'Data Updated Successfully');
          location.href = "#/FobMasterDashboard";
        });
      }

    };


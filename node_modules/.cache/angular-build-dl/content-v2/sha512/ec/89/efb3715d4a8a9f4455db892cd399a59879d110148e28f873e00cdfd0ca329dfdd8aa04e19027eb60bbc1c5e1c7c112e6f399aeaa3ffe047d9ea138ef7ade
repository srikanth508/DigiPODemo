          if (result.value) {
            this.pomservice.DeleteBuyingCompanyMaster(id).subscribe(res => {
              let test = res;
              this.Getbuyingcompany();
            });
            sweetalert2__WEBPACK_IMPORTED_MODULE_3___default.a.fire("Deleted!", "Your Data has been deleted.", "success");

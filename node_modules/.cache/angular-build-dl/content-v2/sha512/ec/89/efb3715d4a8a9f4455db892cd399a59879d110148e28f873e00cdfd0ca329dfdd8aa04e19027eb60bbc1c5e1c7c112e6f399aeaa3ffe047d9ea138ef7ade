        this.spinner.show();
        this.GetUserList();
        this.GetDepartment();
      }

      GetUserList() {
        this.pomservice.GetPoGenaratorLogins().subscribe(data => {
          this.spinner.hide();
          this.userslist = data;
          this.dummuserlist = data;
          this.count = this.userslist.length;
        }, error => {});
      }

      GetDepartment() {
        this.pomservice.GetDepartmentMaster().subscribe(data => {
          this.departmentlist = data;
        });

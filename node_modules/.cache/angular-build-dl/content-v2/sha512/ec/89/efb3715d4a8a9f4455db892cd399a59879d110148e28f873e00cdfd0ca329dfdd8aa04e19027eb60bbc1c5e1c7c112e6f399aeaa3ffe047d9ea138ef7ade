    const EXCEL_TYPE = 'application/vnd.openxmlformats-officedocument.spreadsheetml.sheet;charset=UTF-8';
    const EXCEL_EXTENSION = '.xlsx';
    let AccountingCodeMasterDashboardComponent = class AccountingCodeMasterDashboardComponent {
      constructor(PomsService, activatedroute, spinner) {
        this.PomsService = PomsService;
        this.activatedroute = activatedroute;
        this.spinner = spinner;
        this.p = 1;
      }

      ngOnInit() {
        this.spinner.show();
        this.GetAccountingCodeMaster();
      }

      GetAccountingCodeMaster() {
        this.PomsService.GetAccountingCodeMaster().subscribe(data => {
          this.spinner.hide();
          this.AccountingList = data;
          this.dummaccountlist = data;
          this.count = this.AccountingList.length;
        });
      }

      GetPoTypeID(even) {
        if (even.target.value != 0) {
          this.AccountingList = this.dummaccountlist.filter(x => x.poTypeID == even.target.value);
          this.count = this.AccountingList.length;
        } else {
          this.GetAccountingCodeMaster();
        }
      } // public Delete(ID) {
      //   this.PomsService.DeleteAccountingCodeMaster(ID).subscribe(res => {
      //     this.GetAccountingCodeMaster();
      //     Swal.fire(
      //       "success",
      //       "Accounting Code Deleted Successfully",
      //       "success"
      //     );
      //   });
      // }


      Delete(id) {
        sweetalert2__WEBPACK_IMPORTED_MODULE_3___default.a.fire({
          title: "Are you sure?",
          text: "You will not be able to recover this Data!",
          icon: "warning",
          showCancelButton: true,
          confirmButtonText: "Yes, Delete it!",
          cancelButtonText: "No, keep it"
        }).then(result => {
          if (result.value) {
            this.PomsService.DeleteAccountingCodeMaster(id).subscribe(res => {
              let test = res;
              this.GetAccountingCodeMaster();
            });
            sweetalert2__WEBPACK_IMPORTED_MODULE_3___default.a.fire("Deleted!", "Your Data has been Deleted.", "success");
          } else if (result.dismiss === sweetalert2__WEBPACK_IMPORTED_MODULE_3___default.a.DismissReason.cancel) {
            sweetalert2__WEBPACK_IMPORTED_MODULE_3___default.a.fire("Cancelled", "Your Data  is safe :)", "error");
            this.GetAccountingCodeMaster();
          }
        });
      }

      getglmasterexcel() {
        let hhh = this.tableToJson(document.getElementById('POREPORTS'));
        this.exportAsExcelFile(hhh, "Accounting Master");
      }

      tableToJson(table) {
        var data = []; // first row needs to be headers

        var headers = [];

        for (var i = 0; i < table.rows[0].cells.length; i++) {
          headers[i] = table.rows[0].cells[i].innerHTML.toUpperCase().replace(/ /gi, '');
        } // go through cells 


        for (var i = 1; i < table.rows.length; i++) {
          var tableRow = table.rows[i];
          var rowData = {};

          for (var j = 0; j < tableRow.cells.length - 1; j++) {
            rowData[headers[j]] = tableRow.cells[j].innerHTML;
          }

          data.push(rowData);
        }

        return data;
      }

      exportAsExcelFile(json, excelFileName) {
        const worksheet = xlsx__WEBPACK_IMPORTED_MODULE_5__["utils"].json_to_sheet(json);
        const workbook = {
          Sheets: {
            'data': worksheet
          },
          SheetNames: ['data']
        };
        const excelBuffer = xlsx__WEBPACK_IMPORTED_MODULE_5__["write"](workbook, {
          bookType: 'xlsx',
          type: 'array'
        });
        this.saveAsExcelFile(excelBuffer, excelFileName);
      }

      saveAsExcelFile(buffer, fileName) {
        const data = new Blob([buffer], {
          type: EXCEL_TYPE
        });
        file_saver__WEBPACK_IMPORTED_MODULE_6__["saveAs"](data, fileName + '_export_' + new Date().getTime() + EXCEL_EXTENSION);
      }

      pageChanged(even) {
        let fgdgfgd = even;
        this.p = even;
      }

    };


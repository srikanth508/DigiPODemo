      }

      UpdateApproveRemarks() {
        var entity = {
          'ID': this.approveorderid,
          'StageOfAccept': this.authostage,
          'Remarks': this.remarks
        };
        this.pomservice.UpdatePurchaseOrderAcceptStatus(entity).subscribe(res => {
          debugger; // let data = res;

          if (res != 0) {
            debugger;
            sweetalert2__WEBPACK_IMPORTED_MODULE_3___default.a.fire('Success', 'Order Approved Successfully');

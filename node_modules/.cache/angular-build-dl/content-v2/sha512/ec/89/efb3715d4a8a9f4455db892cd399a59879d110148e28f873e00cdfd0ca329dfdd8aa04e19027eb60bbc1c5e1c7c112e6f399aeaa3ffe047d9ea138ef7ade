      ngOnInit() {
        // this.spinner.show();
        const format = 'yyyy-MM-dd';
        const myDate = new Date();
        const locale = 'en-US';
        this.todaydate = Object(_angular_common__WEBPACK_IMPORTED_MODULE_7__["formatDate"])(myDate, format, locale);
        this.date = Object(_angular_common__WEBPACK_IMPORTED_MODULE_7__["formatDate"])(myDate, format, locale);
        this.genaratorid = localStorage.getItem('genaratorid');
        this.activatedroute.params.subscribe(params => {
          this.id = params['id'];

          if (this.id != undefined || this.id != null) {
            this.showbit = 0;
            debugger;
            this.GetPurchaseOrder();
            this.GetAuthorizationMaster();
          } else {
            this.showbit = 1;
          }
        });
        this.terms = "";
        this.note = "";
        this.podescription = "";
        this.Getbuyingcompany();
        this.GetSuppliermaster();

        this.enddate = Object(_angular_common__WEBPACK_IMPORTED_MODULE_6__["formatDate"])(lll, format, locale);
        this.GetAuthorizationMaster();
        this.GetSuppliermaster();
        this.Getbuyingcompany();
      }

      GetSuppliermaster() {
        this.pomservice.GetSupplierMaster().subscribe(data => {
          this.supplierlist = data;
        }, error => {});
      }

      Getbuyingcompany() {
        this.pomservice.GetBuyingCompanyMaster().subscribe(data => {
          this.companylist = data;
        }, error => {});
      }

      GetAuthorizationMaster() {
        this.pomservice.GetPurchaseOrderApprovredOrders(this.authorizationid, this.startdate, this.enddate).subscribe(data => {
          this.spinner.hide();
          this.polist = data;
          this.dummpolist = data;
          this.count = this.polist.length;
          this.grandtotal = 0;

          for (let i = 0; i < this.polist.length; i++) {
            this.grandtotal = Number(this.grandtotal) + Number(this.polist[i].totalAmountwithtax);
          }
        }, error => {});
      }

      GetSupplierName(even) {
        this.suppliername = even.target.value;

        if (even.target.value != 0) {
          this.suppliername = even.target.value;
          this.polist = this.dummpolist.filter(x => x.supplierName == this.suppliername);
          this.grandtotal = 0;

          for (let i = 0; i < this.polist.length; i++) {
            this.grandtotal = Number(this.grandtotal) + Number(this.polist[i].totalAmountwithtax);
          }

          this.count = this.polist.length;
        } else {
          this.GetAuthorizationMaster();
        }
      }

      selectedDate(data) {

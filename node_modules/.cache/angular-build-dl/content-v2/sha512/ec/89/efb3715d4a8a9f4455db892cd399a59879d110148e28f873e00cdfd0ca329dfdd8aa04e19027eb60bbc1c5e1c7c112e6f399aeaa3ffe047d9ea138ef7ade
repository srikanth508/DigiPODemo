      }

      insertmessage() {
        var entity = {
          'ChatID': this.chatid,
          'SenderName': this.username,
          'Messages': this.message
        };
        this.PomsService.InsertNotificatin_Messages(entity).subscribe(data => {
          this.GetMessages();
          sweetalert2__WEBPACK_IMPORTED_MODULE_3___default.a.fire('Completed', 'Message Sent Successfully');
          this.message = "";
        });
      } // public insertmessage() {

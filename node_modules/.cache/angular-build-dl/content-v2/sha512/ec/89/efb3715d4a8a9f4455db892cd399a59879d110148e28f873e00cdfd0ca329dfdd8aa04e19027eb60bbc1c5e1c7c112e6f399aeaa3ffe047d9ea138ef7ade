        if (valpassword == false) {
          this.pp = 1;
        } else {
          if (this.departmentid == 0 || this.departmentid == "" || this.departmentid == undefined) {
            sweetalert2__WEBPACK_IMPORTED_MODULE_3___default.a.fire("Please Select Department");

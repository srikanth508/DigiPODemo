          sweetalert2__WEBPACK_IMPORTED_MODULE_3___default.a.fire("The Po Can not be sent to the Vendor as there is no email associated with it");
        }
      }

      clear() {
        debugger;
        this.purchaseorderid = "";
        this.PoNumber = "";
        this.ordertype = "", this.duedate = "", this.terms = "", this.fobcode = "", this.shipvia = "", this.date = "", this.accountno = "", this.billtoname = "", this.billtoaddress = "", this.billtoemail = "", this.suppliernme = "", this.suppliercode = "", this.supplierstreet = "", this.supplieraddress = "", this.note = "", this.shiptoname = "", this.shiptostrret = "", this.shiptoaddress = "", this.dept = "", this.potype = "", this.buyername = "", this.totalamount = "", this.totaltaxamount = "", this.totalAmountwithtax = "", this.billtocity = "", this.billtostate = "", this.billtozipcode = "", this.supplierstate = "", this.suppliercity = "", this.supplierzipcode = "", this.shiptocity = "", this.shiptostate = "", this.shiptozipcode = "", this.Amendementpo = "", this.instruction = "", this.authorizer = "", this.vendoremail = "", this.Authorizeremail = "", this.primaryphoneno = "", this.faxtelephone = "";

    let SupplierItemsDashComponent = class SupplierItemsDashComponent {
      constructor(pomservice, activatedroute) {
        this.pomservice = pomservice;
        this.activatedroute = activatedroute;
      }

      ngOnInit() {
        this.GetSupplyItems();
        this.pomservice.GetCategoryMaster().subscribe(data => {
          this.CategoryList = data;
        });
        this.pomservice.GetSubCategoryMaster().subscribe(data => {
          this.dummsubcategory = data;
        });
      }

      GetSupplyItems() {
        this.pomservice.GetSupplerMasterItems().subscribe(data => {
          this.itemlist = data;
          this.dummitelist = data;
          this.count = this.itemlist.length;
        }, error => {});
      }

      GetCategoryName(even) {
        this.categoryname = even.target.value;

        if (even.target.value != 0) {
          this.itemlist = this.dummitelist.filter(x => x.categoryName == this.categoryname);
          this.subCategoryList = this.dummsubcategory.filter(x => x.categoryName == this.categoryname);
          this.count = this.itemlist.length;
        } else {
          this.GetSupplyItems();
        }
      }

      GetSubCategoryName(even) {
        this.subcategoryname = even.target.value;

        if (even.target.value != 0) {
          this.itemlist = this.dummitelist.filter(x => x.subCategoryName == this.subcategoryname);
          this.count = this.itemlist.length;
        } else {
          this.GetSupplyItems();
        }
      }

      DeleteItems(id) {
        sweetalert2__WEBPACK_IMPORTED_MODULE_2___default.a.fire({
          title: "Are you sure?",
          text: "You will not be able to recover this  Item!",
          icon: "warning",
          showCancelButton: true,
          confirmButtonText: "Yes, delete it!",
          cancelButtonText: "No, keep it"
        }).then(result => {
          if (result.value) {
            this.pomservice.DeleteSupplerMasterItems(id).subscribe(data => {
              if (data != null || data != undefined) {
                sweetalert2__WEBPACK_IMPORTED_MODULE_2___default.a.fire("Deleted!", "Deleted Successfully", "success");
                this.GetSupplyItems();
              }
            });
          } else if (result.dismiss === sweetalert2__WEBPACK_IMPORTED_MODULE_2___default.a.DismissReason.cancel) {
            sweetalert2__WEBPACK_IMPORTED_MODULE_2___default.a.fire("Cancelled", "Your Item is safe :)", "error");
          }
        });
      }

      getglmasterexcel() {
        let hhh = this.tableToJson(document.getElementById('POREPORTS'));
        this.exportAsExcelFile(hhh, "Supplier Items");
      }

      tableToJson(table) {
        var data = []; // first row needs to be headers

        var headers = [];

        for (var i = 0; i < table.rows[0].cells.length; i++) {
          headers[i] = table.rows[0].cells[i].innerHTML.toUpperCase().replace(/ /gi, '');
        } // go through cells 


        for (var i = 1; i < table.rows.length; i++) {
          var tableRow = table.rows[i];
          var rowData = {};

          for (var j = 0; j < tableRow.cells.length - 1; j++) {
            rowData[headers[j]] = tableRow.cells[j].innerHTML;
          }

          data.push(rowData);
        }

        return data;
      }

      exportAsExcelFile(json, excelFileName) {

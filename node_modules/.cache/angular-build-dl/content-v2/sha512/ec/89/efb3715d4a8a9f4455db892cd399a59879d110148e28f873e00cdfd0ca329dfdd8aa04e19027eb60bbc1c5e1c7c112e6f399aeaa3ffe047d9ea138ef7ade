    let SupplierLoginDashComponent = class SupplierLoginDashComponent {
      constructor(PomsService, spinner) {
        this.PomsService = PomsService;
        this.spinner = spinner;
      }

      ngOnInit() {
        this.spinner.show();
        this.GetSupplierLogins();
        this.GetDepartment();
      }

      GetSupplierLogins() {
        this.PomsService.GetSupplierLogins().subscribe(data => {
          this.spinner.hide();
          this.supplierlogins = data;
          this.count = this.supplierlogins.length;
        });
      }

      GetDepartment() {
        this.PomsService.GetDepartmentMaster().subscribe(data => {
          this.departmentlist = data;
        });

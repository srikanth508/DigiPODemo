    let TaxAdditionsComponent = class TaxAdditionsComponent {
      constructor(PomsService, activatedroute) {
        this.PomsService = PomsService;
        this.activatedroute = activatedroute;
      }

      ngOnInit() {
        this.activatedroute.params.subscribe(params => {
          this.TaxID = params['id'];

          if (this.TaxID == undefined) {
            this.saveupdatebtn = 0;
          } else {
            this.saveupdatebtn = 1;
            this.PomsService.GetTaxMaster().subscribe(data => {
              this.categorylist = data;
              this.categorylist = this.categorylist.filter(x => x.id == this.TaxID);
              this.TaxCode = this.categorylist[0].taxCode;
              this.TaxPercentage = this.categorylist[0].taxPercentage;
              this.description = this.categorylist[0].description;
              this.countryid = this.categorylist[0].countryID;
              this.stateid = this.categorylist[0].stateID;
              this.cityid = this.categorylist[0].cityID;
              this.getcountrymaster();
              this.GetStateMasterbycountryid();
              this.GetCityMasterbyStateid();
            });
          }
        });
        this.stateid = 0;
        this.countryid = 0;
        this.cityid = 0;
        this.getcountrymaster();
      }

      getcountrymaster() {
        this.PomsService.GetCountryMaster().subscribe(data => {
          this.countrylist = data;
        }, error => {});
      }

      GetCountryID(even) {
        this.countryid = even.target.value;
        this.GetStateMasterbycountryid();
      }

      GetStateMasterbycountryid() {
        this.PomsService.GetStateMasterByCountryID(this.countryid).subscribe(data => {
          this.statelist = data;
        }, error => {});
      }

      GetStateID(even) {
        this.stateid = even.target.value;
        this.GetCityMasterbyStateid();
      }

      GetCityMasterbyStateid() {
        this.PomsService.GetCityMasterByStateID(this.stateid).subscribe(data => {
          this.citylist = data;
        }, error => {});
      }

      GetCityID(even) {
        this.cityid = even.target.value;
      }

      InsertTaxMaster() {
        var Entity = {
          "TaxCode": this.TaxCode,
          "TaxPercentage": this.TaxPercentage,
          'Description': this.description,
          'CountryID': this.countryid,
          'StateID': this.stateid,
          'CityID': this.cityid
        };
        this.PomsService.InsertTaxMaster(Entity).subscribe(data => {
          if (data != undefined) {
            sweetalert2__WEBPACK_IMPORTED_MODULE_3___default.a.fire('success', 'Tax Saved Successfully', 'success');
            location.href = "#/TaxAdditionsDashboard";
          }
        });
      }

      UpdateTaxMaster() {
        var Entity = {
          "ID": this.TaxID,
          "TaxCode": this.TaxCode,
          "TaxPercentage": this.TaxPercentage,
          "Description": this.description,
          'CountryID': this.countryid,
          'StateID': this.stateid,
          'CityID': this.cityid
        };
        this.PomsService.UpdateTaxMaster(Entity).subscribe(data => {
          if (data != undefined) {
            sweetalert2__WEBPACK_IMPORTED_MODULE_3___default.a.fire('success', 'Tax Updated Successfully', 'success');
            location.href = "#/TaxAdditionsDashboard";
          }
        });
      }

    };

    TaxAdditionsComponent.ctorParameters = () => [{

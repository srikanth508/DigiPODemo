              if (data != 0) {
                sweetalert2__WEBPACK_IMPORTED_MODULE_3___default.a.fire("Completed", "Details Added Successfully");
                location.href = "#/PoGenaratorLoginDash";
                this.pp = 0;
              } else {
                sweetalert2__WEBPACK_IMPORTED_MODULE_3___default.a.fire("Error", "UserName Already Exists");
                location.href = "#/PoGenaratorLoginDash";
                this.pp = 0;
              }
            });
          }
        }
      }

      GetDepartment() {
        this.pomservice.GetDepartmentMaster().subscribe(data => {
          this.departmentlist = data;
        });
      }

      GetDepartmentID(even) {
        this.departmentid = even.target.value;
      }


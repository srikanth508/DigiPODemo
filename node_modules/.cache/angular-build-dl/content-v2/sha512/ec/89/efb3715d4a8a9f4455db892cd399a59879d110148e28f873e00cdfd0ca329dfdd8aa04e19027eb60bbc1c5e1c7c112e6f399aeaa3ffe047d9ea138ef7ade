        this.pomservice.GetBuyerMaster().subscribe(data => {
          this.buyerlist = data;
          let buy = this.buyerlist.filter(x => x.id == this.id);
          this.buyercode = buy[0].buyerCode, this.buyername = buy[0].buyerName, this.primaryphoneno = buy[0].primaryPhoneNo, this.secondaryhoneno = buy[0].secondaryPhoneNo, this.faxtelephone = buy[0].faxTelephone, this.buyingcompanyid = buy[0].buyingCompanyID, this.countryid = buy[0].countryID, this.cityid = buy[0].cityID, this.stateid = buy[0].stateID, this.Getbuyingcompany();
          this.getcountrymaster();
          this.GetStateMasterbycountryid();
          this.GetCityMasterbyStateid();
        }, error => {});
      }


    let AccountingCodeMasterComponent = class AccountingCodeMasterComponent {
      constructor(PomsService, activatedroute) {
        this.PomsService = PomsService;
        this.activatedroute = activatedroute;
      }

      ngOnInit() {
        this.activatedroute.params.subscribe(params => {
          this.AccountingCodeID = params['id'];

          if (this.AccountingCodeID == undefined) {
            this.saveupdatebtn = 0;
          } else {
            this.saveupdatebtn = 1;
            this.PomsService.GetAccountingCodeMaster().subscribe(data => {
              this.categorylist = data;
              this.categorylist = this.categorylist.filter(x => x.id == this.AccountingCodeID);
              this.AccountingCode = this.categorylist[0].accountingCode;
              this.Description = this.categorylist[0].accountingDescription;
              this.potypeid = this.categorylist[0].poTypeID;
            });
          }
        });
        this.potypeid = "";
      }

      GetPoTypeID(even) {
        this.potypeid = even.target.value;
      }

      InsertAccountingCodeMaster() {
        if (this.potypeid == 0) {
          sweetalert2__WEBPACK_IMPORTED_MODULE_3___default.a.fire('Please Select PO Type');
        } else {
          var Entity = {
            "AccountingCode": this.AccountingCode,
            "AccountingDescription": this.Description,
            "PoTypeID": this.potypeid
          };
          this.PomsService.InsertAccountingCodeMaster(Entity).subscribe(data => {
            if (data != undefined) {
              sweetalert2__WEBPACK_IMPORTED_MODULE_3___default.a.fire('success', 'Accounting Code Master Saved Successfully', 'success');
              location.href = "#/AccountingCodeMasterDashboard";
            }
          });
        }
      }

      UpdateAccountingCodeMaster() {
        var Entity = {
          "ID": this.AccountingCodeID,
          "AccountingCode": this.AccountingCode,
          "AccountingDescription": this.Description,
          "PoTypeID": this.potypeid
        };
        this.PomsService.UpdateAccountingCodeMaster(Entity).subscribe(data => {
          if (data != undefined) {
            sweetalert2__WEBPACK_IMPORTED_MODULE_3___default.a.fire('success', 'Accounting Code Master updated Successfully', 'success');
            location.href = "#/AccountingCodeMasterDashboard";
          }
        });
      }

    };

    AccountingCodeMasterComponent.ctorParameters = () => [{

    let AuthologindashComponent = class AuthologindashComponent {
      constructor(PomsService, spinner) {
        this.PomsService = PomsService;
        this.spinner = spinner;
      }

      ngOnInit() {
        this.spinner.show();
        this.GetAuthologin();
        this.GetDepartment();
      }

      GetAuthologin() {
        this.PomsService.GetAuthorizationLogins().subscribe(data => {
          this.spinner.hide();
          this.authologin = data;
          this.dummauthologin = data;
          this.count = this.authologin.length;
        });
      }

      GetDepartment() {
        this.PomsService.GetDepartmentMaster().subscribe(data => {
          this.departmentlist = data;
        });
      }

      GetDepartmentName(even) {
        this.departmentname = even.target.value;

        if (even.target.value != 0) {
          this.authologin = this.dummauthologin.filter(x => x.departmentName == this.departmentname);
          this.count = this.authologin.length;
        } else {
          this.GetAuthologin();
        }
      }

      DisabeleAuthorizationLogins(id) {
        sweetalert2__WEBPACK_IMPORTED_MODULE_3___default.a.fire({
          title: 'Are you sure?',
          text: 'You Want To Disable This Person!',
          icon: 'warning',
          showCancelButton: true,
          confirmButtonText: 'Yes, Disable it!',
          cancelButtonText: 'No, keep it'
        }).then(result => {
          if (result.value) {
            this.PomsService.DisabeleAuthorizationLogins(id).subscribe(res => {
              let test = res;
              this.GetAuthologin();
            });
            sweetalert2__WEBPACK_IMPORTED_MODULE_3___default.a.fire('Disabled!', 'Person has Disabled.', 'success');
          } else if (result.dismiss === sweetalert2__WEBPACK_IMPORTED_MODULE_3___default.a.DismissReason.cancel) {
            sweetalert2__WEBPACK_IMPORTED_MODULE_3___default.a.fire('Cancelled', 'Person  is safe :)', 'error');
            this.GetAuthologin();
          }
        });
      }

      EnableAuthorizationLogins(id) {
        sweetalert2__WEBPACK_IMPORTED_MODULE_3___default.a.fire({
          title: 'Are you sure?',
          text: 'You Want To Enable This Person!',
          icon: 'warning',
          showCancelButton: true,
          confirmButtonText: 'Yes, Enable it!',
          cancelButtonText: 'No, keep it'
        }).then(result => {
          if (result.value) {
            this.PomsService.EnableAuthorizationLogins(id).subscribe(res => {
              let test = res;
              this.GetAuthologin();
            });
            sweetalert2__WEBPACK_IMPORTED_MODULE_3___default.a.fire('Enabled!', 'Person has Enabled.', 'success');
          } else if (result.dismiss === sweetalert2__WEBPACK_IMPORTED_MODULE_3___default.a.DismissReason.cancel) {
            sweetalert2__WEBPACK_IMPORTED_MODULE_3___default.a.fire('Cancelled', 'Person  is safe :)', 'error');
            this.GetAuthologin();
          }
        });
      }

      getglmasterexcel() {
        let hhh = this.tableToJson(document.getElementById('POREPORTS'));
        this.exportAsExcelFile(hhh, "Authorization Logins");
      }

      tableToJson(table) {
        var data = []; // first row needs to be headers

        var headers = [];

        for (var i = 0; i < table.rows[0].cells.length; i++) {
          headers[i] = table.rows[0].cells[i].innerHTML.toUpperCase().replace(/ /gi, '');
        } // go through cells 


        for (var i = 1; i < table.rows.length; i++) {
          var tableRow = table.rows[i];
          var rowData = {};

          for (var j = 0; j < tableRow.cells.length - 1; j++) {
            rowData[headers[j]] = tableRow.cells[j].innerHTML;
          }

          data.push(rowData);
        }

        return data;
      }

      exportAsExcelFile(json, excelFileName) {
        const worksheet = xlsx__WEBPACK_IMPORTED_MODULE_4__["utils"].json_to_sheet(json);
        const workbook = {
          Sheets: {
            'data': worksheet
          },
          SheetNames: ['data']
        };
        const excelBuffer = xlsx__WEBPACK_IMPORTED_MODULE_4__["write"](workbook, {
          bookType: 'xlsx',
          type: 'array'
        });
        this.saveAsExcelFile(excelBuffer, excelFileName);
      }

      saveAsExcelFile(buffer, fileName) {
        const data = new Blob([buffer], {
          type: EXCEL_TYPE
        });
        file_saver__WEBPACK_IMPORTED_MODULE_5__["saveAs"](data, fileName + '_export_' + new Date().getTime() + EXCEL_EXTENSION);
      }

      GetChangePassword(id, password) {
        this.id = id;
        this.Password = password;
      }

      updatepassword() {
        var valpassword = this.PomsService.strongpassword(this.Password);

        if (valpassword == false) {
          this.pp = 1;
        } else {
          var entity = {
            'ID': this.id,
            'Password': this.Password
          };
          this.PomsService.UpdateAuthorizationLoginsPassword(entity).subscribe(data => {
            let res = data;
            sweetalert2__WEBPACK_IMPORTED_MODULE_3___default.a.fire('Password Changed Successfully');
            document.getElementById('close').click();
            this.pp = 0;
            this.GetAuthologin();
          });
        }
      }

    };


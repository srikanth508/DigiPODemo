      GetDepartmentName(even) {
        this.departmentname = even.target.value;

        if (even.target.value != 0) {
          this.userslist = this.dummuserlist.filter(x => x.departmentName == this.departmentname);
          this.count = this.userslist.length;
        } else {
          this.GetUserList();

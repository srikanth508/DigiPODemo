    let EditSupplierItemMasterComponent = class EditSupplierItemMasterComponent {
      constructor(pomservice, activatedroute) {
        this.pomservice = pomservice;
        this.activatedroute = activatedroute;
      }

      ngOnInit() {
        this.activatedroute.params.subscribe(params => {
          this.ID = params["id"];
          this.getcategorymaster();
          this.GetItemMaster();
          this.GetSupplyItems();
          this.GetSuppliermaster();
        });
      }

      GetSuppliermaster() {
        this.pomservice.GetSupplierMaster().subscribe(data => {
          this.supplierlist = data;
        }, error => {});
      }

      getcategorymaster() {
        this.pomservice.GetCategoryMaster().subscribe(data => {
          this.CategoryList = data;
        });
      }

      getCategoryID(even) {
        this.Categoryid = even.target.value;
        this.pomservice.GetSubCategoryMaster().subscribe(data => {
          this.SubcategoryList = data;
          var List = this.SubcategoryList.filter(x => x.categoryID == this.Categoryid);
          this.SubcategoryList = List;
        });
      }

      GetItemMaster() {
        this.pomservice.GetItemMaster().subscribe(data => {
          this.ItemList = data;
        });
      }

      GetSupplyItems() {
        this.pomservice.GetSupplerMasterItems().subscribe(data => {
          this.details = data;
          var list = this.details.filter(x => x.id == this.ID);
          this.supplierid = list[0].supplierID;
          this.Categoryid = list[0].categoryID;
          this.subcategoryid = list[0].subCategoryID;
          this.itemid = list[0].itemID;
          this.nettotal = list[0].netTotal;
          this.taxPercentage = list[0].taxPercentage;
          this.grandtotal = list[0].grandTotal;
          this.taxid = list[0].taxID;
          this.pomservice.GetSubCategoryMaster().subscribe(data => {
            this.SubcategoryList = data;
            var List = this.SubcategoryList.filter(x => x.categoryID == this.Categoryid);
            this.SubcategoryList = List;
          });
        }, error => {});
      }

      GetsubcategoryID(even) {
        this.subcategoryid = even.target.value;
      }

      GetSupplierID(even) {
        this.supplierid = even.target.value;
        var list = this.supplierlist.filter(x => x.id == this.supplierid);
        this.suppliername = list[0].supplierName;
      }

      GetItemID(even) {
        this.itemid = even.target.value;
        var list = this.ItemList.filter(x => x.id == this.itemid);
        this.cost = list[0].cost, this.taxPercentage = list[0].taxPercentage, this.taxid = list[0].taxid, this.itemName = list[0].itemName, this.grandtotal = this.cost * this.taxPercentage / 100 + this.cost;
      }

      UpdateDetails() {
        debugger;
        var entity = {
          ID: this.ID,
          SupplierID: this.supplierid,
          CategoryID: this.Categoryid,
          SubCategoryID: this.subcategoryid,
          ItemID: this.itemid,
          NetTotal: this.nettotal,
          GrandTotal: this.grandtotal,
          TaxID: this.taxid
        };
        this.pomservice.UpdateSupplerMasterItems(entity).subscribe(data => {
          debugger;

          if (data != undefined || data != null) {
            sweetalert2__WEBPACK_IMPORTED_MODULE_3___default.a.fire("Updated Successfully");
            location.href = "#/SupplierItemsDash";
          }
        });
      }

    };

    EditSupplierItemMasterComponent.ctorParameters = () => [{
      type: _poms_service__WEBPACK_IMPORTED_MODULE_2__["PomsService"]

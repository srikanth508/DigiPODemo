      sendmail1() {
        debugger; // "C:\\inetpub\\wwwroot\\digiPOAPI\\Images\\PO\\20210326085908PO.pdf"

        this.emailattchementurl.push("D:\\inetpub\\wwwroot\\digiPOAPI\\Images\\PO\\Termsandconditions.pdf");
        var entity = {
          'emailto': this.vendoremail,
          'emailsubject': 'PO_from Tops:' + this.PoNumber,
          'emailbody': "Dear (" + this.suppliernme + "),<br><br><br>" + "Attached is the Tops Markets Purchase Order along with applicable Terms and Conditions. Feel free to contact us if you have any questions or concerns.<br>" + "<br><br><br>Thank You  <br>" + this.authorizer + "<br>Tops Purchasing Dept<br>direct line: " + this.primaryphoneno + "<br>fax: " + this.faxtelephone + " <br>" + this.Authorizeremail,
          'attachmenturl': this.emailattchementurl // this.emailattchementurl

        };
        debugger;
        this.pomservice.sendemail1(entity).subscribe(data => {
          debugger;

          if (data == 'Success') {
            this.message = data;
            sweetalert2__WEBPACK_IMPORTED_MODULE_3___default.a.fire("Sent Mail Successfully");
            this.InsertEmailSuccessLog();
            document.getElementById('Close').click();
            this.emailattchementurl.length = 0;
            this.emailattchementurl = [];
            this.spinner.hide();
          } else {
            this.message = data;
            sweetalert2__WEBPACK_IMPORTED_MODULE_3___default.a.fire("Please Contact Administrator. Email can not be sent");
            this.inserterrorlog();
            this.emailattchementurl.length = 0;
            this.emailattchementurl = [];
            this.spinner.hide();
          }
        });
      }

      inserterrorlog() {
        debugger;
        var entity = {
          'PO_Number': this.PoNumber,
          'AuthorizerID': this.authorizationid,
          'Message': this.message
        };
        this.pomservice.InsertEmailError_log(entity).subscribe(data => {});
      }

      InsertEmailSuccessLog() {
        debugger;

    "./node_modules/@angular/router/fesm2015/router.js");

    let InstructionMasterComponent = class InstructionMasterComponent {
      constructor(pomservice, activatedroute) {
        this.pomservice = pomservice;
        this.activatedroute = activatedroute;
      }

      ngOnInit() {
        this.activatedroute.params.subscribe(params => {
          this.id = params['id'];

          if (this.id != undefined || this.id != null) {
            this.showbit = 0;
            this.GetInstructionMaster();
          } else {
            this.showbit = 1;
          }
        });
      }

      insertdetails() {
        var entity = {
          'InstructionCode': this.instructioncode,
          'Instruction': this.instruction
        };
        this.pomservice.InsertInstructionMaster(entity).subscribe(data => {
          if (data != 0) {
            sweetalert2__WEBPACK_IMPORTED_MODULE_3___default.a.fire('Completed', 'Data Added Successfully');
            location.href = "#/InstructionMasterDashboard";
            this.clear();
          }
        });
      }

      clear() {
        this.instruction = "", this.instructioncode = "";
      }

      GetInstructionMaster() {
        this.pomservice.GetInstructionMaster().subscribe(data => {
          this.instructionlist = data;
          var insta = this.instructionlist.filter(x => x.id == this.id);
          this.instructioncode = insta[0].instructionCode, this.instruction = insta[0].instruction;
        }, error => {});
      }

      Updatedetails() {
        var entity = {
          'ID': this.id,
          'InstructionCode': this.instructioncode,
          'Instruction': this.instruction
        };
        this.pomservice.UpdateInstructionMaster(entity).subscribe(res => {
          let data = res;
          sweetalert2__WEBPACK_IMPORTED_MODULE_3___default.a.fire('Success', 'Data Updated Successfully');
          location.href = "#/InstructionMasterDashboard";
        });
      }

    };


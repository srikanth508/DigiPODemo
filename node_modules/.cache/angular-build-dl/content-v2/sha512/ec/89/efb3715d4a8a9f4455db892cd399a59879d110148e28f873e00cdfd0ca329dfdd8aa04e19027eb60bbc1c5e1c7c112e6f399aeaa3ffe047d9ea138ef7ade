    let BuyingCompanyMasterDashboardComponent = class BuyingCompanyMasterDashboardComponent {
      constructor(pomservice, spinner) {
        this.pomservice = pomservice;
        this.spinner = spinner;
        this.p = 1;
      }

      ngOnInit() {
        this.spinner.show();
        this.Getbuyingcompany();
        this.getcountrymaster();
      }

      Getbuyingcompany() {
        this.pomservice.GetBuyingCompanyMaster().subscribe(data => {
          this.spinner.hide();
          this.companylist = data;
          this.dummcompanylist = data;
          this.count = this.companylist.length;
        }, error => {});
      }

      getcountrymaster() {
        this.pomservice.GetCountryMaster().subscribe(data => {
          this.countrylist = data;
        }, error => {});
      }

      GetCountryName(even) {
        let list = even.target.value.split(",");
        this.countryname = list[0];
        this.countryid = list[1];

        if (even.target.value != 0) {
          this.companylist = this.dummcompanylist.filter(x => x.countryID == this.countryid);
          this.count = this.companylist.length;
        } else {
          this.Getbuyingcompany();
        }

        this.GetStateMasterbycountryid();
      }

      GetStateMasterbycountryid() {
        this.pomservice.GetStateMasterByCountryID(this.countryid).subscribe(data => {
          this.statelist = data;
        }, error => {});
      }

      GetStateName(even) {
        let list1 = even.target.value.split(",");
        this.statename = list1[0];
        this.stateid = list1[1];

        if (even.target.value != 0) {
          this.companylist = this.dummcompanylist.filter(x => x.stateID == this.stateid);
          this.count = this.companylist.length;
        } else {
          this.Getbuyingcompany();
          this.cityid = 0;
          this.stateid = 0;
          this.statelist.length = 0;
          this.citylist.length = 0;
          this.countryid = 0;
          this.countrylist.length = 0;
          this.getcountrymaster();
        }

        this.GetCityMasterbyStateid();
      }

      GetCityMasterbyStateid() {
        this.pomservice.GetCityMasterByStateID(this.stateid).subscribe(data => {
          this.citylist = data;
        }, error => {});
      }

      GetCityName(even) {
        let list2 = even.target.value.split(",");
        this.cityname = list2[0];
        this.cityid = list2[1];

        if (even.target.value != 0) {
          this.companylist = this.dummcompanylist.filter(x => x.cityID == this.cityid);

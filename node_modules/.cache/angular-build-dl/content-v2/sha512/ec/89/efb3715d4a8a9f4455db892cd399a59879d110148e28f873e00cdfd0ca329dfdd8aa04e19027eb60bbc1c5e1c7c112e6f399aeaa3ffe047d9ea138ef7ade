    const EXCEL_TYPE = 'application/vnd.openxmlformats-officedocument.spreadsheetml.sheet;charset=UTF-8';
    const EXCEL_EXTENSION = '.xlsx';
    let TenderDashComponent = class TenderDashComponent {
      constructor(pomservice, spinner) {
        this.pomservice = pomservice;
        this.spinner = spinner;
        this.SDate = new Date();
        this.EDate = new Date();
        this.p = 1;
      }

      ngOnInit() {
        // this.spinner.show();
        this.spinner.show();
        this.genaratorid = localStorage.getItem('genaratorid');
        this.options = {
          theme: 'default',
          range: 'tm',
          dayNames: ['Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat', 'Sun'],
          presetNames: ['This Month', 'Last Month', 'This Week', 'Last Week', 'This Year', 'Last Year', 'Start', 'End'],
          dateFormat: 'yyyy/MM/dd',
          outputFormat: 'YYYY/MM/DD',
          startOfWeek: 1
        };
        let date = new Date();
        let hours = date.getHours();
        let minutes = date.getMinutes();
        let newformat = hours >= 12 ? 'PM' : 'AM'; // Find current hour in AM-PM Format 

        hours = hours % 12; // To display "0" as "12" 

        hours = hours ? hours : 12;
        minutes = minutes < 10 ? 0 + minutes : minutes;
        var kkk = this.SDate.setDate(this.SDate.getDate() - 300);
        var lll = this.EDate.setDate(this.EDate.getDate() + 300);
        const format = 'yyyy-MM-dd';
        const myDate = new Date();
        const locale = 'en-US';
        this.todaydate = Object(_angular_common__WEBPACK_IMPORTED_MODULE_4__["formatDate"])(myDate, format, locale);
        this.startdate = Object(_angular_common__WEBPACK_IMPORTED_MODULE_4__["formatDate"])(kkk, format, locale);
        this.enddate = Object(_angular_common__WEBPACK_IMPORTED_MODULE_4__["formatDate"])(lll, format, locale);
        this.GetPurchaseOrder();
        this.GetDepartmentMater();
        this.status = "Raised";
        this.GetSuppliermaster();
        this.Getbuyingcompany();
        this.pomservice.GetOrderTypeMaster().subscribe(data => {
          this.ordertypelist = data;
        }, error => {});
      }


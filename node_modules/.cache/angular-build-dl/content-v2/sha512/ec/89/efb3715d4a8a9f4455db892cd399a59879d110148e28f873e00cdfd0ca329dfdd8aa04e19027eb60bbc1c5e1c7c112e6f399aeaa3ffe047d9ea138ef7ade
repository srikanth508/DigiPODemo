    let MessagesComponent = class MessagesComponent {
      constructor(PomsService) {
        this.PomsService = PomsService;
        this.SDate = new Date();
        this.EDate = new Date();
      }

      ngOnInit() {
        this.options = {
          theme: 'default',
          range: 'tm',
          dayNames: ['Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat', 'Sun'],
          presetNames: ['This Month', 'Last Month', 'This Week', 'Last Week', 'This Year', 'Last Year', 'Start', 'End'],
          dateFormat: 'yyyy/MM/dd',
          outputFormat: 'YYYY/MM/DD',
          startOfWeek: 1
        };
        let date = new Date();
        let hours = date.getHours();
        let minutes = date.getMinutes();
        let newformat = hours >= 12 ? 'PM' : 'AM'; // Find current hour in AM-PM Format 

        hours = hours % 12; // To display "0" as "12" 

        hours = hours ? hours : 12;
        minutes = minutes < 10 ? 0 + minutes : minutes;
        var kkk = this.SDate.setDate(this.SDate.getDate() - 300);
        var lll = this.EDate.setDate(this.EDate.getDate() + 300);
        const format = 'yyyy-MM-dd';
        const myDate = new Date();
        const locale = 'en-US';
        this.todaydate = Object(_angular_common__WEBPACK_IMPORTED_MODULE_4__["formatDate"])(myDate, format, locale);
        this.startdate = Object(_angular_common__WEBPACK_IMPORTED_MODULE_4__["formatDate"])(kkk, format, locale);
        this.enddate = Object(_angular_common__WEBPACK_IMPORTED_MODULE_4__["formatDate"])(lll, format, locale);
        this.userid = localStorage.getItem('UserID');
        this.username = localStorage.getItem('user');
        this.GetMessages();
        this.PomsService.GetDistinctPONumberForMessagessssssss().subscribe(data => {
          this.dummponos = data;
          this.ponos = this.dummponos.filter(x => x.poGenaratorID == this.userid || x.supplierID == this.userid);
        });
      }

      selectedDate(data) {
        var sdate = data.split('-');
        this.startdate = sdate[0];
        this.enddate = sdate[1];
        this.PomsService.GetNotificatin_MessagesByDate(this.userid, this.startdate, this.enddate).subscribe(data => {
          this.messages = data;
          this.msgdummlist = data;
          this.count = this.messages.length;
        });
      }

      GetMessages() {
        this.PomsService.GetNotificationMessages(this.userid).subscribe(data => {
          this.messages = data;
          this.msgdummlist = data;
          this.count = this.messages.length;
        });
      }

      GetPoNos(even) {
        this.pono = even.target.value;

        if (even.target.value != 0) {
          this.messages = this.msgdummlist.filter(x => x.poNo == this.pono);
          this.count = this.messages.length;
        } else {
          this.GetMessages();
        }
      }

      GetChatID(chatid) {
        this.chatid = chatid; // this.sendername = senderName;
        // this.pogenaratorid = pOGenaratorID;
        // this.supplierid = supplierID
        // this.poid = pOid;
